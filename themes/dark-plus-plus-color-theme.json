{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark++",
  "type": "dark",
  "include": "./dark_plus.json",
  "colors": {
    "editor.background": "#1e1e21",
    "activityBar.background": "#33333a",
    "debugToolBar.background": "#33333a",
    "menu.background": "#25252a",
    "editorGroupHeader.tabsBackground": "#25252a",
    "editorHoverWidget.background": "#25252a",
    "editorInlayHint.foreground": "#ccc",
    "editorSuggestWidget.background": "#25252a",
    "editorWidget.background": "#25252a",
    "notifications.background": "#25252a",
    "peekViewResult.background": "#25252a",
    "quickInput.background": "#25252a",
    "sideBar.background": "#25252a",
    "welcomePage.tileBackground": "#25252a",
    "checkbox.background": "#3C3C40",
    "checkbox.border": "#3C3C40",
    "dropdown.background": "#3C3C40",
    "dropdown.border": "#3C3C40",
    "input.background": "#3C3C40",
    "settings.checkboxBackground": "#3C3C40",
    "settings.checkboxBorder": "#3C3C40",
    "settings.dropdownBackground": "#3C3C40",
    "settings.dropdownBorder": "#3C3C40",
    "settings.numberInputBackground": "#3C3C40",
    "settings.textInputBackground": "#3C3C40",
    "titleBar.activeBackground": "#304670",
    "titleBar.inactiveBackground": "#3C3C44",
    "welcomePage.progress.background": "#3C3C40",
    "tab.border": "#25252a",
    "tab.inactiveBackground": "#2f2f36",
